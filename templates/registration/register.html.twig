{% extends 'base.html.twig' %}

{% block title %}Orchestra - Inscription{% endblock %}

{% block body %}
    <h1>Inscription</h1>

    {{ form_errors(registration) }}

    {{ form_start(registration) }}
        {{ form_row(registration.email) }}
        {{ form_row(registration.plainPassword, {
            label: 'Mot de passe'
        }) }}
        {{ form_row(registration.pseudo) }}
    {{ form_end(registration) }}
{% endblock %}
